{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFprod1999"
		},
		"LS_sql_to_park_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_sql_to_park'",
			"defaultValue": "Integrated Security=True;Data Source=LAPTOP-QL9VQEA3;Initial Catalog=salesEcom"
		},
		"LS_sql_to_park_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_sql_to_park'"
		},
		"LS_sql_to_park_sink_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_sql_to_park_sink'"
		},
		"LS_restAPI_to_park_sink_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_restAPI_to_park_sink'"
		},
		"LS_sql_to_park_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Rondy Thomas K"
		},
		"LS_sql_to_park_sink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlssidevcom007.dfs.core.windows.net/"
		},
		"LS_restAPI_to_park_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://retoolapi.dev/oC5U82/data"
		},
		"LS_restAPI_to_park_sink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlssidevcom007.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFprod1999_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_sql_to_park_sink_accountKey": {
						"value": "[parameters('LS_sql_to_park_sink_accountKey')]"
					},
					"LS_restAPI_to_park_sink_accountKey": {
						"value": "[parameters('LS_restAPI_to_park_sink_accountKey')]"
					},
					"LS_sql_to_park_connectionString": {
						"value": "[parameters('LS_sql_to_park_connectionString')]"
					},
					"LS_sql_to_park_password": {
						"value": "[parameters('LS_sql_to_park_password')]"
					},
					"LS_sql_to_park_sink_properties_typeProperties_url": {
						"value": "[parameters('LS_sql_to_park_sink_properties_typeProperties_url')]"
					},
					"LS_restAPI_to_park_properties_typeProperties_url": {
						"value": "[parameters('LS_restAPI_to_park_properties_typeProperties_url')]"
					},
					"LS_restAPI_to_park_sink_properties_typeProperties_url": {
						"value": "[parameters('LS_restAPI_to_park_sink_properties_typeProperties_url')]"
					},
					"LS_sql_to_park_properties_typeProperties_userName": {
						"value": "[parameters('LS_sql_to_park_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}